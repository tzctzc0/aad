{
	"manifest_version": 3,
	"name": "aad",
	"version": "1.16",
	"permissions": ["downloads", "contextMenus", "offscreen", "storage"],
	"host_permissions": ["https://*.live/*", "https://*.la/*"],
	"action": {},
	"background": {
		"service_worker": "dist/sw.js",
		"type": "module"
	},
	"options_ui": {
		"page": "options.html",
		"open_in_tab": false
	},
	"content_scripts": [
		{
			"matches": ["https://*.live/*"],
			"js": ["dist/content.js"]
		}
	],
	"web_accessible_resources": [
		{
			"matches": ["https://*.live/*"],
			"resources": ["style.min.css"]
		}
	]
}
